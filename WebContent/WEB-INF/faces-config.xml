<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
  <application>
    <resource-bundle>
      <base-name>com.clarity.messages</base-name>
      <var>msgs</var>
    </resource-bundle>
  </application>
  <managed-bean>
  	<managed-bean-name>mapService1</managed-bean-name>
  	<managed-bean-class>com.clarity.MapService</managed-bean-class>
  	<managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>weatherService1</managed-bean-name>
  	<managed-bean-class>com.clarity.WeatherService</managed-bean-class>
  	<managed-bean-scope>application</managed-bean-scope>
  </managed-bean>              
</faces-config>